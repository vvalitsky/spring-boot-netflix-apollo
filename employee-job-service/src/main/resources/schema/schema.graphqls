type Employee @key(fields: "id")
@extends {
    id: Int! @external
    "Employee job info"
    employeeJob: EmployeeJob
}

"Employee job info"
type EmployeeJob {
    "Employee job identifier"
    id: Int!
    "Employee job name"
    name: String!
    "Employee job address"
    address: String!
    "Employee job excpirience"
    experience: Int!
}