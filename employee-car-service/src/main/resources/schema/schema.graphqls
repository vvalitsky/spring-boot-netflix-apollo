type Employee @key(fields: "id")
@extends {
    id: Int! @external
    "Employee cars info"
    cars: [EmployeeCar!]!
}

"Employee car information"
type EmployeeCar {
    "Employee car identifier"
    id: Int!
    "Employee car model"
    model: String!
    "Employee car brand"
    brand: String!
    "Employee car release date"
    releaseDate: DateTime!
}

scalar DateTime